#include<bits/stdc++.h>
using namespace std;
int main()
{int n,x,a[1000],t,p;
cin>>t;
for(int j=1;j<=t;j++)
{
    int count=0,sum=0;
    cin>>n>>p>>x;
for(int i=0;i<n;i++)
cin>>a[i];
sort(a,a+n);
for(int i=0;i<n;i++)
{sum+=a[i];
    if(sum <=x && count<p)
    {count++;
    }
    else
        break ;
}
    cout<< "Case "<<j<<": "<< count <<endl;



}

    return 0;
}
