#include<bits/stdc++.h>
using namespace std;
int main()
{int t,n,x,a[100000],sum=0;
cin>>t;
while(t--)
{
    sum=0;
       cin>>n;
    for(int i=0;i<n;i++)
        cin>>a[i];

        for(int i=0;i<n;i++)
        for(int j=i-1;j>=0;j--)
        if(a[i]>=a[j])
            sum++;
            cout<<sum<<endl;

}
    return 0;
}
