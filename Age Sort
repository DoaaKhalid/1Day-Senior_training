#include<iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
int n,x;
while (cin>>n,n)
{
    vector<int> v;
    for(int i=0;i<n;i++)
        {cin>>x;
            v.push_back(x);}
    sort(v.begin(),v.end());
    for(int i=0;i<n;i++)
      {
          if(i!=n-1)
        cout<<v[i]<<" ";
        else
            cout<<v[i];
        }
       cout<<endl;
}

	return 0;
}
