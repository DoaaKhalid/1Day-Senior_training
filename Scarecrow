#include<bits/stdc++.h>
using namespace std;
int main()
{ string s;
int t,x;
cin>>t;
for(int j=1;j<=t;j++)
    {int count=0;
    cin>>x>>s;
    for(int i=0;i<=s.size();i++)
    {
        if(s[i]=='.')
        {
        s[i]=s[i+1]=s[i+2]='#';
            count++;
        }
    }
    
     cout<< "Case "<<j<<": "<< count <<endl;
    }

    return 0;
}
