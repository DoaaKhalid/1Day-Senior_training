#include<bits/stdc++.h>
using namespace std;
long long s(long long x){
    long long sum=0;
    while(x){
        sum+=x%10;
        x/=10;}
    return sum;
    }
int main()
{
long long  n,x,y;
cin>>n;
x=sqrt(n);
for(long long i=x;i<n;i++)
{
    if(i*i+s(i)*i==n)
    {
    cout<<i<<endl;
    return 0;}
}
cout<<"-1"<<endl;
    return 0;
}
