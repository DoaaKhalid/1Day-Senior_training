#include<iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,a[1005],a2[1005];
    cin>>n;
    for(int i=0;i<n;i++)
    cin>>a[i];
    sort(a,a+n);
    int i,j;
    i=0;
    j=n-1;
    while (i<=j)
    {
       cout<<a[i]<<" ";
        i++;
        if(i<=j)
           {
            cout<<a[j]<<" ";
            j--;
            }
    }

	return 0;
}
