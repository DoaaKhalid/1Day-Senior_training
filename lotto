#include<bits/stdc++.h>
using namespace std;
int main()
{
int n,k,a[20];
bool flag=false;
while (cin>>n,n)
{
    if(flag)
        cout<<endl;
    flag=true;
    for(int i=0;i<n;i++)
    cin>>a[i];
for(int i=0;i<n-5;i++)
for(int j=i+1;j<n-4;j++)
for(int l=j+1;l<n-3;l++)
for(int m=l+1;m<n-2;m++)
for(int k=m+1;k<n-1;k++)
for(int d=k+1;d<n;d++)

    cout<<a[i]<<" "<<a[j]<<" " <<a[l]<<" " <<a[m]<<" "<<a[k]<<" "<<a[d]<<endl;


}
    return 0;
}
