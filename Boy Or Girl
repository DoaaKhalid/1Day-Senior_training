
#include <iostream>
#include <string>
#include <set>

using namespace std;
int main() {
	int x,y;
	string s;
	set<char>s1;
	cin >> s;
	for (int i = 0;i < s.size();i++)
		s1.insert(s[i]);
	x = s1.size();
	y = x % 2;
	if (y == 0)
		cout << "CHAT WITH HER!" << endl;
	else
		cout << "IGNORE HIM!" << endl;
	
	return 0;
}
